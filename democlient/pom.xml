        <relativePath/> <!-- lookup parent from repository -->
    </parent>
    <groupId>cn.ppphuang</groupId>
    <artifactId>democlient</artifactId>
    <version>0.0.1-SNAPSHOT</version>
    <name>democlient</name>
    <description>democlient</description>
    <properties>
